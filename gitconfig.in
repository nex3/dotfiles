[user]
        name = Natalie Weizenbaum
        email = nweiz@google.com
[core]
        excludesfile=~/.gitignore.global
[alias]
        st = status
        co = !hub checkout
        ci = commit
        cp = cherry-pick
        fp = format-patch
        bs = bisect
        br = branch
        rb = rebase
        push = push --tags
        slog = log --pretty=oneline --abbrev-commit
        from = !sh -c 'git slog --no-merges "$1.."' --
        sdiff = diff --stat=150,130
        sshow = show --stat=150,130
        msg = commit --allow-empty -m
        prp = !hub pull-request --push
        L = !git-land
[color]
        ui = auto
[push]
	default = upstream
[url "sso://code/"]
	insteadof = https://code.googlesource.com/
[pull]
	rebase = false
[protocol "sso"]
	allow = always
